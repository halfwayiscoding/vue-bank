<svg width="192" height="192" viewBox="0 0 192 192" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bankGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- 背景圆角矩形 -->
  <rect x="16" y="16" width="160" height="160" rx="32" ry="32" fill="url(#bankGradient)"/>
  
  <!-- 银行图标 -->
  <g transform="translate(96,96)">
    <!-- 银行建筑主体 -->
    <rect x="-40" y="-20" width="80" height="40" fill="white" opacity="0.9"/>
    
    <!-- 银行柱子 -->
    <rect x="-35" y="-15" width="8" height="30" fill="white"/>
    <rect x="-15" y="-15" width="8" height="30" fill="white"/>
    <rect x="5" y="-15" width="8" height="30" fill="white"/>
    <rect x="25" y="-15" width="8" height="30" fill="white"/>
    
    <!-- 银行屋顶 -->
    <polygon points="-45,-25 0,-40 45,-25" fill="white"/>
    
    <!-- 银行门 -->
    <rect x="-8" y="0" width="16" height="20" fill="#667eea"/>
    
    <!-- 货币符号 -->
    <circle cx="0" cy="-5" r="12" fill="none" stroke="white" stroke-width="2"/>
    <text x="0" y="0" text-anchor="middle" fill="white" font-size="14" font-weight="bold">¥</text>
  </g>
</svg>